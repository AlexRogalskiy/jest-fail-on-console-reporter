  },
  "devDependencies": {
    "@jest/types": "24.8.0",
    "@types/jest": "24.0.16",
    "@types/node": "12.6.8",
    "jest": "24.8.0",
    "prettier": "1.18.2",
    "rimraf": "2.6.3",
